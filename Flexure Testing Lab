import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
from scipy.stats import linregress

df = pd.read_csv('Homework/Nick_Noah.csv')
time = df['Time (s)']
disp = df['Displacement (mm)']
F = df['Force (kN)']
L = 150
b = 25
h = 2.75
stress = np.array((3/2)*(F*L/(b*h**2)))
strain = np.array((6*disp*h)/(L**2))

plt.figure()
plt.plot(time, disp, '*')
plt.xlabel('Time (s)')
plt.ylabel('Displacement (mm)')

plt.figure()
plt.plot(time, F, '*')
plt.xlabel('Time (s)')
plt.ylabel('Force (kN)')

plt.figure()
plt.plot(disp, F, '*')
plt.xlabel('Displacement (mm)')
plt.ylabel('Force (kN)')

plt.figure()
plt.plot(strain, stress, '*', label='Data Points')
plt.xlabel('Strain')
plt.ylabel('Stress (kN/mm²)')
slope, intercept, r_value, p_value, std_err = linregress(strain[2000:2750], stress[2000:2750])
print(f"Modulus of Elasticity is approximately: {slope} kN/mm²")
print(f"R-squared: {r_value**2}")
plt.show()
